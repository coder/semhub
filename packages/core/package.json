    "hono": "^4.6.5",
    "octokit": "^4.0.2",
    "openai": "^4.69.0",
    "postgres": "^3.4.4",
    "ulidx": "^2.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@tsconfig/node20": "20.1.4",
    "drizzle-kit": "^0.24.2",
    "vitest": "^2.1.3"
